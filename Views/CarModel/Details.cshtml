@model SigortaTakipSistemi.Models.CarModels

@{
    ViewData["Title"] = "Model Detayları";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Model Detayları</h1>

<div>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.CarBrandName)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CarBrand.Name)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-primary" asp-action="Edit" asp-route-id="@Model.Id">Güncelle</a>
    <a class="btn btn-dark" asp-action="Index">Listeye Geri Dön</a>
</div>
